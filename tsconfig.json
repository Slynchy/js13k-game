    "outDir": "./dist/",
    "baseUrl": "./src",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es6",
    "jsx": "react-native",
    "allowJs": true,
    "sourceMap": true,
    "paths": {
      "tsengine": ["../lib/tsengine"]
    },
    "typeRoots": [ "./types", "./node_modules/@types"]
  }
}
